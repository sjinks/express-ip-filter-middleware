    "eslint-formatter-gha": "^1.4.3",
    "express": "^5.0.0",
    "node-reporter-gha": "^2.0.2",
    "nyc": "^17.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjinks/express-ip-filter-middleware.git"
